{
  "rules": {
    "indent": 2,
    "quotes": [2, "single"],
    "no-undef": 2,
    "no-use-before-define": [2, "nofunc"],
    "space-before-function-paren": [2, {
      "anonymous": "always",
      "named": "never"
    }],
    "space-before-blocks": [2, "always"]
  },
  "env": {
      "node": true,
      "mocha": true,
      "browser": true
  }
}
