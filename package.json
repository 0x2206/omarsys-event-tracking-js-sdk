{
  "name": "cherrytech-event-tracking-js-sdk",
  "description": "JavaScript client library for CherryTech Event Tracking system",
  "version": "1.0.0",
  "private": true,
  "main": "src/main.js",
  "dependencies": {
    "axios": "0.8.1",
    "browser-cookies": "1.0.8",
    "browser-cookies-shim": "0.0.1",
    "es6-promise": "3.0.2",
    "node-uuid": "1.4.3"
  },
  "devDependencies": {
    "ct-distribute": "git+ssh://git@stash.cherrytech.com:7999/bow/ct-distribute.git#0.1.0",
    "eslint": "1.10.3",
    "exports-loader": "0.6.2",
    "git-scripts": "0.2.1",
    "istanbul-instrumenter-loader": "0.1.3",
    "karma": "0.13.15",
    "karma-coverage": "0.5.3",
    "karma-junit-reporter": "0.3.8",
    "karma-mocha": "0.2.1",
    "karma-phantomjs-launcher": "0.2.1",
    "karma-spec-reporter": "0.0.23",
    "karma-threshold-reporter": "0.1.15",
    "karma-webpack": "1.7.0",
    "mocha": "2.3.4",
    "phantomjs": "1.9.19",
    "phantomjs-polyfill": "0.0.1",
    "uglify-js": "2.6.1",
    "webpack": "1.12.9"
  },
  "scripts": {
    "build": "npm run build:lib && npm run build:loader",
    "build:lib": "webpack",
    "build:loader": "uglifyjs -c -m -o ./dist/loader.min.js ./src/loader.js",
    "clean": "rm -rf ./dist",
    "distribute": "distribute",
    "lint": "eslint ./src",
    "prebuild": "npm test && npm run clean && mkdir ./dist",
    "pretest": "npm run lint",
    "test": "npm run test:node && npm run test:browser",
    "test:browser": "karma start",
    "test:node": "mocha -R spec src/lib/*.spec.js"
  },
  "engines": {
    "node": ">=0.12.0"
  },
  "browser": {
    "browser-cookies-shim": "browser-cookies"
  },
  "git": {
    "scripts": {
      "pre-push": "npm test"
    }
  }
}
