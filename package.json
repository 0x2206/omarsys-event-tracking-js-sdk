{
  "name": "cherrytech-event-tracking-js-sdk",
  "description": "JavaScript client library for CherryTech Event Tracking system",
  "version": "0.0.0",
  "private": true,
  "main": "src/main.js",
  "dependencies": {
    "axios": "^0.5.4",
    "bower": "^1.4.1",
    "cookie-cutter": "^0.1.1",
    "cookie-cutter-shim": "file:src/shims/cookie-cutter-shim",
    "node-uuid": "^1.4.3"
  },
  "devDependencies": {
    "bluebird": "^2.10.0",
    "browserify": "^10.2.6",
    "browserify-istanbul": "^0.2.1",
    "eslint": "^0.24.1",
    "git-scripts": "^0.2.1",
    "karma": "^0.13.0",
    "karma-browserify": "^4.2.1",
    "karma-coverage": "^0.4.2",
    "karma-junit-reporter": "^0.3.3",
    "karma-mocha": "^0.2.0",
    "karma-phantomjs-launcher": "^0.2.0",
    "karma-spec-reporter": "0.0.20",
    "karma-threshold-reporter": "^0.1.15",
    "mocha": "^2.2.5",
    "nconf": "^0.7.1",
    "phantomjs": "^1.9.17",
    "phantomjs-polyfill": "0.0.1",
    "replace": "^0.3.0",
    "semver": "^5.0.1",
    "uglify-js": "^2.4.23",
    "uglifyify": "^3.0.1"
  },
  "scripts": {
    "build": "npm run build:lib && npm run build:loader && bin/replacer",
    "build:lib": "browserify -t uglifyify src/main.js | uglifyjs -c -m > dist/tracking.min.js",
    "build:loader": "uglifyjs src/loader.js -o dist/loader.min.js -c -m",
    "clean": "rm -rf ./dist/*",
    "distribute": "bin/distribute",
    "lint": "eslint src/",
    "prebuild": "npm test && npm run clean && mkdir -p ./dist",
    "pretest": "npm run lint",
    "test": "npm run test:node && npm run test:browser",
    "test:browser": "node_modules/karma/bin/karma start karma.conf.js",
    "test:node": "node_modules/.bin/mocha -R spec ./src/lib/*.spec.js"
  },
  "engines": {
    "node": ">=0.12.0"
  },
  "browser": {
    "cookie-cutter-shim": "cookie-cutter"
  },
  "git": {
    "scripts": {
      "pre-push": "npm test"
    }
  }
}
